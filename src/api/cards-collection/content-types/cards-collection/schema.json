{
  "kind": "collectionType",
  "collectionName": "cards_collections",
  "info": {
    "singularName": "cards-collection",
    "pluralName": "cards-collections",
    "displayName": "Cards Collections",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "cardName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "cardImage": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "mobileCardImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "cardCategory": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "joiningInfo": {
      "type": "component",
      "repeatable": true,
      "component": "cards-collection-info.cards-collections",
      "required": true
    },
    "landing_page": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::landing-page.landing-page",
      "inversedBy": "cards_collections"
    }
  }
}
